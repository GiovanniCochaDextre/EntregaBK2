<h1>{{title}}</h1>

{{!-- <form action="/api/session/register" method="post"> --}}
<form action="/api/users/register" method="post">
    <div>
        <div>
            <label for="first_name">Nombre:</label>
            <br />
            <input type="text" id="first_name" name="first_name" placeholder="Nombres" />
        </div>

        <div>
            <label for="last_name">Apellidos:</label>
            <br />
            <input type="text" id="last_name" name="last_name" placeholder="Apellidos" />
        </div>

        <div>
            <label for="email">Email:</label>
            <br />
            <input type="email" id="email" name="email" placeholder="email@mail.com" />
        </div>

        <div>
            <label for="age">Edad:</label>
            <br />
            <input type="text" id="age" name="age" placeholder="20" />
        </div>

        <div>
            <label for="role">Rol:</label>
            <br />
            <input type="text" id="role" name="role" placeholder="user" />
        </div>

        <div>
            <label for="password">Password:</label>
            <br />
            <input type="password" id="password" name="password" placeholder="12345" />
        </div>

        <br />
        <button type="submit">Registrar</button>
    </div>
</form>
